<h1>Listing educations</h1>

<table>
  <tr>
    <th>Degree type</th>
    <th>Degree subject</th>
    <th>School</th>
    <th>School location</th>
    <th>Graduation date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @educations.each do |education| %>
  <tr>
    <td><%= education.degree_type %></td>
    <td><%= education.degree_subject %></td>
    <td><%= education.school %></td>
    <td><%= education.school_location %></td>
    <td><%= education.graduation_date %></td>
    <td><%= link_to 'Show', education %></td>
    <td><%= link_to 'Edit', edit_education_path(education) %></td>
    <td><%= link_to 'Destroy', education, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Education', new_education_path %>
<%= link_to 'Back', '/' %>